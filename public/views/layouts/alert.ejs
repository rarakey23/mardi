<% if (success_msg != '') { %>
<script>
  Swal.fire({
    icon: 'success',
    title: `Success!`,
    text: "<%- success_msg %>",
    customClass: {
      confirmButton: 'btn btn-success'
    }
  });
</script>
<% } %>

<% if (error_msg != '') { %>
<script>
  Swal.fire({
    icon: 'error',
    title: `Error!`,
    text: "<%- error_msg %>",
    customClass: {
      confirmButton: 'btn btn-success'
    }
  });
</script>k
<% } %>